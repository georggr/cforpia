                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/ggeorgiadis/ontologies/2025/C-FORPIA#hasOutcome"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#DPIAOutcomeStatus"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
